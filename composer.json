{
  "name": "ics/ssi-bundle",
  "description": "Bundle security extention",
  "type": "symfony-bundle",
  "version": "0.2.6",
  "license": "MIT",
  "authors": [
    {
      "name": "David Dutas",
      "email": "david.dutas@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ICS\\SsiBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ICS\\SsiBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/config": "^5",
    "symfony/dependency-injection": "^4.4|^5",
    "symfony/event-dispatcher": "^4.4|^5",
    "symfony/framework-bundle": "^5",
    "symfony/intl": "^4.4|^5",
    "symfony/string": "^5",
    "symfony/translation": "^4.4|^5",
    "symfony/twig-bundle": "^4.4|^5",
    "symfony/http-client": "^5",
    "symfony/orm-pack": "^2.1",
    "easycorp/easyadmin-bundle": "^3.2",
    "symfony/console": "^5",
    "symfony/monolog-bundle": "^3.6",
    "symfony/security-bundle": "^5",
    "ics/navigation-bundle": "^0",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "ics/dashboard-bundle": "^0",
    "stevenmaguire/oauth2-keycloak": "^2.2",
    "knpuniversity/oauth2-client-bundle": "^2.8"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.30"
  }
}
